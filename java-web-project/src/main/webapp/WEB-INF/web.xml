<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>TestFilter</filter-name>
  	<display-name>TestFilter</display-name>
  	<description></description>
  	<filter-class>com.highkeen.training.jwp.filter.TestFilter</filter-class>
  </filter>
  <filter>
  	<filter-name>CountFilter</filter-name>
  	<display-name>CountFilter</display-name>
  	<description></description>
  	<filter-class>com.highkeen.training.jwp.filter.CountFilter</filter-class>
  </filter>
 
  <filter-mapping>
  	<filter-name>TestFilter</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
  	<filter-name>CountFilter</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
  
 <!--  <welcome-file-list>
  	<welcome-file>hello.jsp</welcome-file>
  </welcome-file-list> -->
  <listener>
  	<listener-class>com.highkeen.training.jwp.ApplicationStartupListener</listener-class>
  </listener>
  <servlet>
  	<servlet-name>HelloServlet</servlet-name>
  	<display-name>HelloServlet</display-name>
  	<description></description>
  	<servlet-class>com.highkeen.training.jwp.HelloServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RequestStart</servlet-name>
  	<display-name>RequestStart</display-name>
  	<description></description>
  	<servlet-class>com.highkeen.training.jwp.RequestStart</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ApplicationScopeTest</servlet-name>
  	<display-name>ApplicationScopeTest</display-name>
  	<description></description>
  	<servlet-class>com.highkeen.training.jwp.ApplicationScopeTest</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>StartSession</servlet-name>
  	<display-name>StartSession</display-name>
  	<description></description>
  	<servlet-class>com.highkeen.training.jwp.StartSession</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SessionTest</servlet-name>
  	<display-name>SessionTest</display-name>
  	<description></description>
  	<servlet-class>com.highkeen.training.jwp.SessionTest</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ShowName</servlet-name>
  	<display-name>ShowName</display-name>
  	<description></description>
  	<servlet-class>com.highkeen.training.jwp.ShowName</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EmployeeServlet</servlet-name>
  	<display-name>EmployeeServlet</display-name>
  	<description></description>
  	<servlet-class>com.highkeen.training.jwp.EmployeeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>HelloServlet</servlet-name>
  	<url-pattern>/HelloServlet.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RequestStart</servlet-name>
  	<url-pattern>/RequestStart.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ApplicationScopeTest</servlet-name>
  	<url-pattern>/ApplicationScopeTest.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>StartSession</servlet-name>
  	<url-pattern>/StartSession.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SessionTest</servlet-name>
  	<url-pattern>/SessionTest.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ShowName</servlet-name>
  	<url-pattern>/ShowName.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EmployeeServlet</servlet-name>
  	<url-pattern>/employees</url-pattern>
  </servlet-mapping>
</web-app>
